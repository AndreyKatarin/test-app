<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="600" minWidth="800" xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1" fx:controller="av.katarin.client.FormController">
    <TableView VBox.vgrow="ALWAYS" fx:id="tableView">
        <columns>
            <TableColumn text="Дата" fx:id="date">
            </TableColumn>
            <TableColumn text="№ Договора" fx:id="documentNumber">
            </TableColumn>
            <TableColumn text="Дата обновления" fx:id="updateDate">
            </TableColumn>
            <TableColumn text="Актуален" fx:id="isActive">
            </TableColumn>
        </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
    </TableView>
</VBox>
